{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./ng-dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "include": [
      "ng-src/**/*.ts",
      "ng-e2e/**/*.ts"
  ],
}
