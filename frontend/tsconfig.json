{
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "outDir": "./ng-dist/out-tsc",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es2015",
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ],
    "typeRoots": [
      "node_modules/typess"
    ],
    "lib": [
      "es2017.object",
      "es2016",
      "dom"
    ],
    "module": "esnext"
  }
}
